    {"pattern": "classified program (disclosure|breach|leak)", "pattern_type": "regex", "risk_level": "high", "description": "Classified program information disclosure"},
    {"pattern": "ITAR (violation|investigation|penalty)", "pattern_type": "regex", "risk_level": "high", "description": "ITAR export control violation"},
