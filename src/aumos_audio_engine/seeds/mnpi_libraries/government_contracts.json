    {"pattern": "sole source (award|justification|contract)", "pattern_type": "regex", "risk_level": "high", "description": "Sole source contract award"},
    {"pattern": "GAO (protest|decision|sustained|denied)", "pattern_type": "regex", "risk_level": "high", "description": "GAO bid protest outcome"},
    {"pattern": "bid protest", "pattern_type": "keyword", "risk_level": "high", "description": "Government bid protest filed"},
    {"pattern": "debarment (notice|proceeding|proposed)", "pattern_type": "regex", "risk_level": "high", "description": "Government contractor debarment"},
    {"pattern": "suspension (notice|proceeding|proposed)", "pattern_type": "regex", "risk_level": "high", "description": "Government contractor suspension"},
    {"pattern": "cost overrun (disclosed|identified|reported)", "pattern_type": "regex", "risk_level": "high", "description": "Government contract cost overrun"},
    {"pattern": "DCAA (audit|finding|disallowance)", "pattern_type": "regex", "risk_level": "high", "description": "DCAA audit finding"},
