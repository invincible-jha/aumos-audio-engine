    {"pattern": "security clearance (revocation|suspension|denial)", "pattern_type": "regex", "risk_level": "high", "description": "Security clearance action"},
    {"pattern": "DoD (contract|program|budget|cut)", "pattern_type": "regex", "risk_level": "high", "description": "DoD contract or budget event"},
    {"pattern": "continuing resolution", "pattern_type": "keyword", "risk_level": "medium", "description": "Government budget continuing resolution"},
    {"pattern": "sequestration (cut|impact|trigger)", "pattern_type": "regex", "risk_level": "medium", "description": "Sequestration budget impact"},
    {"pattern": "program (cancellation|restructuring|Nunn-McCurdy)", "pattern_type": "regex", "risk_level": "high", "description": "Defense program restructuring"},
